}

@ARTICLE{visionbased2009,
  author = {Cho, Chien-Wen and Chao, Wen-Hung and Lin, Sheng-Huang and Chen,
	You-Yin},
  title = {A vision-based analysis system for gait recognition in patients with
	Parkinson's disease},
  journal = {Expert Syst. Appl.},
  year = {2009},
  volume = {36},
  pages = {7033--7039},
  month = {April},
  abstract = {Recognition of specific Parkinsonian gait patterns is helpful in the
	diagnosis of Parkinson's disease (PD). However, there are few computer-aided
	methods to identify the specific gait patterns of PD. We propose
	a vision-based diagnostic system to aid in recognition of the gait
	patterns of Parkinson's disease. The proposed system utilizes an
	algorithm combining principal component analysis (PCA) with linear
	discriminant analysis (LDA). This scheme not only addresses the high
	data dimensionality problem during image processing but also distinguishes
	different gait categories simultaneously. The feasibility of the
	proposed system for the recognition of PD gait was tested by using
	gait videos of PD and normal subjects. The efficiency of feature
	extraction using PCA and LDA coefficients are also compared. Experimental
	results showed that LDA had a recognition rate for Parkinsonian gait
	of 95.49%, which is higher than the conventional PCA feature extraction
	method. The proposed system is a promising aid in identifying the
	gait of Parkinson's disease patients and can discriminate the gait
	patterns of PD patients and normal people with a very high classification
	rate.},
  acmid = {1498190},
  address = {Tarrytown, NY, USA},
  doi = {10.1016/j.eswa.2008.08.076},
  file = {:C\:\\Users\\leonardo\\Dropbox\\doutorado ufcg\\bibliografia\\parkison\\A vision-based analysis system for gait recognition in patients with Parkinson’s disease - chao 09.pdf:PDF},
  issn = {0957-4174},
  issue = {3},
  keywords = {Gait analysis, Linear discriminant analysis (LDA), Parkinson's disease,
	Principal component analysis (PCA), Vision-based},
  numpages = {7},
  owner = {leonardo},
  publisher = {Pergamon Press, Inc.},
  review = {A maioria dos métodos utilizados para avaliar a DP, necessitam do
	expertise humano, como por exemplo no uso da UPDRS.
	
	
	A análise das características de movimento, documentada por Knutsson
	(1972), demonstra que os pacientes da DP, exibem uma larga variabilidade
	de movimento compara as pessoas normais. Esles caminham mais devagar
	a passos mais curtos e a amplitude de movimento das articulações
	é diminuída. Isso especifica um padrão de movimento, largamente aceito
	como característica proeminente da doença. (McDowell, 1971). Entretando,
	a postura do movimento pode variar de pessoa para pessoa, a avaliação
	de movimento do parkisoniando tende a ser subjetiva e depende fortemente
	do expertis de julgamento do médico (Blin, Ferrandez, & Serratrice,
	1990; Lubik et al., 2006; Melnick, Radtka, & Piper, 2002; Shan et
	al., 2001; Salarian et al., 2004; Sofuwa et al., 2005; Stern et al.,
	1983; Vokaer, Azar, & Beyl, 2003).
	
	
	As a result, engineering-oriented machine learning-based methods have
	attracted more and more attention in this field (Engin et al., 2007;
	Fahrenberg et al., 1997; Makikawa & Iizumi, 1995; Sekine et al.,
	2002; Veltink et al., 1996). Many previous studies have used dc and
	ac accelerometers to assess gait patterns. They classified the accelerometer
	signals into different typesof walking and correlated them with energy
	consumption. Nevertheless, those methods often used a number of sensors,
	causing patient discomfort.
	
	
	The aim of this paper is to discriminate PD patients from normal subjects
	using a vision-based gait analysis approach. The scheme utilizes
	the holistic image of subjects, and extracts and reduces the feature
	space by using PCA and LDA. The meaning of the obtained LDA transformation
	matrix (reduced to a vector in our case) is not only treated as a
