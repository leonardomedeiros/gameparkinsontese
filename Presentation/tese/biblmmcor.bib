@INPROCEEDINGS{6091362,
  author = {El-Gohary, M. and Holmstrom, Lars and Huisinga, J. and King, E. and
	McNames, James and Horak, F.},
  title = {Upper limb joint angle tracking with inertial sensors},
  booktitle = {Engineering in Medicine and Biology Society,EMBC, 2011 Annual International
	Conference of the IEEE},
  year = {2011},
  pages = {5629-5632},
  abstract = {Wearable inertial systems have recently been used to track human movement
	in and outside of the laboratory. Continuous monitoring of human
	movement can provide valuable information relevant to individual's
	level of physical activity and functional ability. Traditionally,
	orientation has been calculated by integrating the angular velocity
	from gyroscopes. However, a small drift in the measured velocity
	leads to large integration errors that grow with time. To compensate
	for that drift, complementary data from accelerometers are normally
