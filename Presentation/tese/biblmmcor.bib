@CONFERENCE{chen11,
  author = {Shervin Amini, Richard Verhoeven, Johan Lukkien, Shudong Chen},
  title = {Toward a Security Model for a Body Sensor Platform},
  booktitle = {2011 IEEE International Conference on Consumer Electronics (ICCE)},
  year = {2011},
  pages = {143 - 144},
  publisher = {IEEE Computer Society},
  abstract = {Body Sensor Networks (BSNs) are used in the ubiquitous healthcare
	to measure human body functions like
	
	vital physiological data (e.g. heart beat). As the privacy-sensitive
	sensor data are transferred over the unreliable wireless
	
	connection and can be shared among various BSN applications, it is
	necessary to protect this data against potential threats. The purpose
	of this paper is to introduce an approach to design a lightweight
	security model with respect to limited resource
	
	constraints of our special purpose sensor platform.},
  owner = {leonardo},
  review = {O projeto VITRUVIUS, pretende desenvolver uma plataforma de sensor
	para o corpo humano, que poassa ser aplicado a diversas redes BSN
	instaladas dinamicamente, pretendendo se conectar a sistemas de retaguarda
	de qualquer plataforma. Os dados capturados peos sensores devem ser
	trafegados por redes sem fio de acordo com os padrões de segurança
	estabelecidos sem alteração ou violação da autenticidade dos dados.
	
	
	Os requisitos de segurança de uma BSN são significativamente diferentes
	de uma rede sensor típica. Contrastando que numa rede sensor típica
	são medidos dados públicos como temperatura, humidade em uma rede
	BSN são coletados informação vitais humanas como o batimento cardíaco
	e a pressão sanguínea.
	
	
	In this paper we present system architecture and we outline potential
	threats to the BSN as part of the overall system. With
	
	respect to the threats, we define a set of security requirements and
	summarize related solutions proposed by state-of-the-art
	
	security protocols in the WSN.
	
	
	In this paper, we consider threats to the BSN with respect to the
	communication channel from sensors to body hub. The
	
	threats are instantiated by the attacks which are effective because
	of associated vulnerabilities (weaknesses) in the BSN.
	
	
	Foram examinados a ocorrência dos seguintes ataques e vulnerabilidades:
	
	1-Wrong data from sensor : This might happen because of a faulty sensor,
	a noisy channel or malicious alteration of
	
	 transmitted messages.
	
	
	2-Data loss
	
	
	
	3-Spoofing of sensor : Unintentionally or maliciously a sensor may
	be attached to a wrong person or a wrong sensor-body hub relationship
	may be established. Furthermore, the attacker may spoof the identity
	of the sensor. Our security method must make such spoofing impossible
	by using authentication.
	
	
	4-Eavesdropping and replay: A malicious attacker might intercept messages
	for inspection or replay. Our security
	
	method, therefore, must encrypt messages and protect them
	
	against the replay
	
	
	III. SECURITY REQUIREMENTS
	
	A. Confidentiality. - Uso de criptografia
	
	B. Authenticity. - Armazenar o MacAddress + a criptografia
	
	C. Replay protection. - Contagem de mensagem e timestamp
	
	D. Tolerance to message loss. - O número de mensagem perdida pode
	ser realizada através de um contador},
  timestamp = {2011.07.13}
}

