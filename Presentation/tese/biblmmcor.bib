@INPROCEEDINGS{gar09,
  author = {Garcia-Morchon, O. and Falck, T. and Heer, T. and Wehrle, K.},
  title = {Security for pervasive medical sensor networks},
  booktitle = {Mobile and Ubiquitous Systems: Networking Services, MobiQuitous,
	2009. MobiQuitous '09. 6th Annual International},
  year = {2009},
  pages = {1 -10},
  month = {july},
  abstract = {Wireless sensor networks are going to allow for ubiquitous health
	monitoring, improving users' well-being, the healthcare system, and
	helping to quickly react on emergency situations. Meeting the strict
	security needs of these ubiquitous medical applications is a big
	challenge, since safety and privacy of medical data has to be guaranteed
	all the way from the sensor nodes to the back-end services, the system
	has to fulfill latency needs, and lots of mobility is expected. In
	this paper, we introduce a deployment model for wireless sensor networks
	for pervasive healthcare based on the concepts of patient area networks
	and medical sensor networks, and propose a complete and efficient
	security framework for them. Our security framework is organized
	into three layers, addressing the operational requirements and security
	needs at the patient area network, medical sensor network and back-end
	levels. We specify how these layers are interconnected with each
	other as well as the needed security mechanisms that allow for the
	efficient and practical deployment of secure pervasive healthcare
	systems based on wireless sensor networks.},
  doi = {10.4108/ICST.MOBIQUITOUS2009.6832},
  keywords = {MSN;PAN;WSN;access control;back-end service;cryptography;emergency
	situation;medical data privacy;medical information system;operational
	requirements;patient area network;pervasive medical sensor network;secure
	pervasive healthcare system;security framework;ubiquitous health
	monitoring safety;ubiquitous medical application;wireless sensor
	network model;authorisation;biomedical communication;cryptography;data
	privacy;health care;medical information systems;patient monitoring;telecommunication
	security;ubiquitous computing;wireless sensor networks;},
  owner = {leonardo},
  review = {autor faz uso dos termos :
	
	PAN: Patient Area Network
	
	MSN: Mobile Sensor Network
	
	
	---------
	
	Para o autor, um framework para Pervasive MSNs e PANs, deve assegurar
	os seguintes serviços de segurança:
	
	Privacidade : refere à proteção da identidade do usuário e segurança
	dos dados a terceiros
	
	
	Confidencialidade: é requerida para proteger as informações médicas
	dos usuários em todo o sistema, desde os nós sensores até os serviços
	de backend.
	
	
	Tanto usuários quanto médicos devem autenticar qualquer acesso ou
	escrita das informações médicas.
	
	
	Esses requisitos de segurança são requeridos pela HITRUST e as diretivas
	legais da HIPAA
	
	==============================
	
	
	O autor propõe um framework para sistemas pervasivos baseados em WSN.
	Combinanando uma uma solução centralizada e distribuída balanceando
	performance e os requisitos de segurança.
	
	
	O framework possui uma arquitetura de segurança em três camadas:
	
	 1. Gerenciamento de segurança da MSN
	
	 2. Gerenciamento de Segurança da PAN
	
	 3. Gerenciamento de segurança dos mecanismos de back-end
	
	
	====
	
	O staff médico pode se comunicar a qualquer PAN, mesmo que tanto PAN
	e médicos estejam em constante movimento.
	
	
	PAN Security Layer: permite que o usuário gerencie a segurança sua
	própria pan, permitindo que ele conceda ou negue o acesso aos dados
	ao interatgir ocm a MSNs e os sistemas de Back End.
	
	
	The Back-end services security layer, assegura que as operações sobre
	a PAN interaja entre as MSN controlando diversas organizações.
	
	O autor assumiu um modelo centralizado e simplificado baseado na infraestrutura
	de chave pública e gerenciamento de certificação de autoridade no
	gerenciamento do sistema de healthcare.
	
	
	MSN Security Layer
	
	-----------------------------
	
	A abordagem básica dessa camada permitiu combinar um handshake de
	acordo e verificação leve de certificados.
	
	
	Security Handshake baseado em criptografia de chave pública requere
	alguns segundos dos sensores e um delay para evitar ataques DoS.
	
	
	PAN Security Layer
	
	----------------------------
	
	A segunda camada de segurança é centrada-no-paciente para controla
	a camada PAN enquanto interage com as MSNs, no instance que controla
	a MSNs associadas aos usuários das PAN asseguranca o link de comunicação
	entre eles.
	
	
	===========================},
  timestamp = {2011.08.26}
}

@INPROCEEDINGS{gobel2010,
  author = {Stefan Gobel and Sandro Hardy and Viktor Wendel and Florian Mehm
	and Ralf Steinmetz},
  title = {Serious games for health: personalized exergames.},
  booktitle = {ACM Multimedia'10},
  year = {2010},
  pages = {1663-1666},
  abstract = {In this paper, we describe a set of personalized exergames which combine
	methods and concepts of serious games, adaptation and personalization,
	authoring and sensor technologies. Compared to existing systems,
	the set of games does not only keep track of the user's vital state,
	but also directly integrates vital parameters into the gameplay and
	supports the training and motivation for sustainable physical activity
	in a playful manner.},
  file = {:C\:\\Users\\leonardo\\Dropbox\\doutorado ufcg\\bibliografia\\jogos\\Serious games for health.pdf:PDF},
  owner = {leonardo},
  review = {Nesse artigo o autor defende que o uso comercial dos jogos de esporte
	e saúde têm-se tornado produtos de sucesso.
	
	Avaliações de trabalhos científicos como os de Baranowski e Kato,
	tem provado efeitos positivos quando os jogos são abordados em direções
	de modificar o comportamento dos jogadores.
	
	
	Trabalhos de Papastergiou, Kretchscman, Kliem e Wiemeyer também indicamo
	efeitos positivos para reabilitação com Wii Sports box ou o potencial
	do Wii Fit como treinamento para a prevenção e reabilitação. Contudo
	esses jogos têm limitações porque não são substitutos adequados dos
	esportes reais, e os obstáculos concretos desses sistemas pode personlizar
	e sustenatar uma motivação sustentávels desses jogos quando aplicados
	à saúde.
	
	
	Baseado nessa observação, esse trabalho prima em levantar as dificuldades
	e efeitos positivos em combinar os jogos sérios de esportes e saúde
	com as tecnologias de sensores, para a personalização e adaptação
	de jogos multiplayer.
	
	
	A partir da aplicação StoryTec, médicos, preparadores físicos e outros
	usuários podem definir o programa de exercício através de programas
	pre-configurados e base de dados das características individuais
	ou do grupo. Elaborando a Story Engine do jogo, essa engine serve
	como um control unit para diversor momentos do jogo, responsável
	em processar o controle de dados vitais restrito a camada de sensores
	da aplicação.
	
	
	Segundo o trabalho de Sinclair [12], os jogos comerciais de exergames,
	não deve utilizá-lo apenas como motivacional para a prática de exercício
	mas também para mensurar seu batimento cardíaco, reconhcer atividades
	via acelerômetros. Um dos problemas é que esses sinais não são considerados
	no gameplay dos exergames. Para esse propósito é proposto uma engine
	adaptativa que recupera os sinais vitais para influenciar diretamente
	no game-play.
	
	
	Personalização e Adaptação
	
	==================
	
	O objetivo dessa abordagem é prover um ambiente adaptado e personalizado
	que considera informação estatístca (treinamento, logica) e dinâmica
	como os sinais vitais que potencialmente altera o comportamento do
	jogador.
	
	
	Conclusão
	
	=======
	
	O objetivo dessa abordagem é prover um ambiente adaptado e personalizado
	que considera informação estatístca (treinamento, logica) e dinâmica
	como os sinais vitais que potencialmente altera o comportamento do
	jogador.
	
	Esse artigo introduz um conjunto de exergames que buscam motivar os
	jogadores a praticarem exercício de forma personalizada através dos
	dados capturados pelos sensores.},
  timestamp = {2011.12.01}
}

