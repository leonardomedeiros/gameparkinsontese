@INPROCEEDINGS{Ballegaard:2008:HEL:1357054.1357336,
  author = {Ballegaard, Stinne Aal{\o}kke and Hansen, Thomas Riisgaard and Kyng,
	Morten},
  title = {Healthcare in everyday life: designing healthcare services for daily
	life},
  booktitle = {Proceeding of the twenty-sixth annual SIGCHI conference on Human
	factors in computing systems},
  year = {2008},
  series = {CHI '08},
  pages = {1807--1816},
  address = {New York, NY, USA},
  publisher = {ACM},
  abstract = {Today the design of most healthcare technology is driven by the considerations
	of healthcare professionals and technology companies. This has several
	benefits, but we argue that there is a need for a supplementary design
	approach on the basis the citizen and his or her everyday life. An
	approach where the main focus is to develop healthcare technology
	that fits the routines of daily life and thus allows the citizens
	to continue with the activities they like and have grown used to
	– also with an aging body or when managing a chronic condition. Thus,
	with this approach it is not just a matter of fixing a health condition,
	more importantly is the matter of sustaining everyday life as a whole.
	This argument is a result from our work – using participatory design
	methods – on the development of supportive healthcare technology
	for elderly people and for diabetic, pregnant women.},
  acmid = {1357336},
  doi = {http://doi.acm.org/10.1145/1357054.1357336},
  isbn = {978-1-60558-011-1},
  keywords = {chronic disease, citizen, design methods, diabetes, elderly, healthcare
	technology, independent living, participatory design, patient, tele-medicine},
  location = {Florence, Italy},
  numpages = {10},
  owner = {leonardo},
  review = {A abordagem dessa solução é desenvolver uma tecnologia de healthcare
	que permita que os cidadãos tenham sua rotina e vida normal e ainda
	conseguir gerenciar e tratar sua doença crônica. Esse argumento foi
	construído através de design centrado no usuário, onde forma avaliados
	pacientes com diabetes e mulheres grávidas. O autor particou de diversos
	workshops e já no início do artigo ele fala que os workshops foram
	evoluindo para : I. Médicos e enfermeira debatiam em como a tecnologia
	móvel e os sensores poderiam ser efetivos para a medicina remota,
	evitando as hospitalizações. II. Como as tecnologias poderiam auxiliar
	na administração de medicamento para idosos, incluindo um balanceamento
	da dieta e monitoramento das condições físicas. III. Como os idosos
	e seus familiares poderiam ter uma vida normal, sem deixar de se
	preocupar com a saúde. Claramente o autor identifica os pontos de
	tensões entre os workshops, para ele os profissionais da saúde e
	os idosos. Para eles os profissionais acham q os idosos querem evitar
	doenças a todo custo. Enquanto que os idosos querem gozar da saúde
	que possuem entre seus 65 e 70 anos. Artigo muito bom, demonstrando
	uma etnografia de 9 meses. Esse artigo foi baseado em 3 workshops
	com o design centrado no usuário. O foco da pesquisa é como desenvolver
	tecnologia de assistencia médica adequado a rotina diária dos usuários,
	buscando propor uma ferramenta para idosos independente e adequada
	a rotina através da ubiquidade. 1o Workshop: foi realizado com médicos
	e enfermeiras que discutiram a respeito como a tecnologia móvel e
	os sensores poderiam habilitar a telemedicina 2o Workshop: homecare
	works, como a tecnologia poderia administrar os medicamentos 3o workshop:
	como os idosos e seus familiares podem ser auxiliados para ter uma
	vida normal através do uso da tecnologia Esses trÊs workshops foram
	úteis para identificar a tensão existente entre os profissionais
	de saúde e idosos. ------------------------------- Tecnologias de
	Monitoramento de Pacientes ============================ As tecnologias
	de monitoramente têm sido oferecidas como uma solução para a remoção
	do paciente do hospital para a sua residência através de serviços
	de home-care. Bastante utilizada para o monitoramente de pacientes
	crônicos e idosos, ainda sofrem rejeição a respeito do design da
	solução. Os sistemas de monitoramento eletrônico tornam possível
	que um filho seja monitorados por seus pais. Ou que um filho possa
	monitorar a alimentação e as horas de sono de um idoso. Os sistemas
	de monitoramento tradicionais estão sendo desenvolvidos para pacientes
	que são vistos como "incapazes" de enteder os dados produzidos. Pelo
	estudo do autor, tanto os: idosos, diabéticos e grávidas; Possuem
	profundo interesse no resultado do que está sendo monitorado. O autor
	encontrou um grande potencial no desenvolvimento de uma tecnologia
	de monitoramento que não tenha apenas utilidade clínica, mas seja
	focado no possível uso de seus usuários, melhorando a qualidade de
	vida do paciente monitorado.},
  timestamp = {2011.05.03}
}

@ARTICLE{baran08,
