@ARTICLE{stacey2011,
  author = {Guy, Stacey and Ratzki-Leewing, Alexandria and Gwadry-Sridhar, Femida},
  title = {Moving Beyond the Stigma: Systematic Review of Video Games and Their
	Potential to Combat Obesity},
  journal = {International Journal of Hypertension},
  year = {2011},
  volume = {2011},
  pages = {1--13},
  abstract = {Increasing epidemic proportions of overweight children in the United
	States presents formidable challenges for education and healthcare.
	Given the popularity and pervasiveness of video gaming culture in
	North American children, the perfect opportunity arises to investigate
	the potential of video games to promote healthful behaviour. Our
	objective was to systematically review the literature for possible
	benefits of active and educational video games targeting diet and
	physical activity in children. A review of English-language journal
	articles from 1998 to 2011 using EMBASE and PubMed was conducted.
	Thirty-four studies concerned with children, video games, physical,
	and/or nutritional outcomes were included. Results of these studies
	that showed some benefit (increased physical activity and nutritional
	knowledge as a result of gaming) demonstrate the possibility of video
	games to combat childhood obesity—looking beyond the stigma attached
	to gaming.},
  citeulike-article-id = {9170458},
  citeulike-linkout-0 = {http://dx.doi.org/10.4061/2011/179124},
  doi = {10.4061/2011/179124},
  issn = {2090-0392},
  keywords = {education\_games, exercise, obesity, review, videogames},
  owner = {leonardo},
  posted-at = {2011-04-18 13:51:21},
  priority = {2},
  review = {Estudo s indicam que 50% dos adolescentes obesos se tornam obesos
	adultos. Obesidade infantil nos Estados Unidos tomou proporção edêmica.A
	obesidade tem se tornado um dos principais problemas pediátricos.
	Estudos indicam que a atividade física e dietas são fatores relacionados
	à perda de peso. Logo, explorar estratégias que reduzam o peso é
	um dos principais objetivos na prática de exercício ou mudança dos
	hábitos alimentares. O departamento de agricultura americano desenvolveu
	um guia de dieta balanceada com atividades físicas. Foi realizada
	uma revisão sistemática nas bases da EMBASE, pubmed em dez 2010.
	BMI,video games, dieta , exergam, para fazer a busca bibliográfica.
	Todos os estudos em exergames apontar para a atividade física. Haddock,
	comparou o oxigênio e energina gasta percebida no uso dos jogos.
	Mellecker examinou a energia gasta e a resposta cardiovascular de
	crianacas setandas ou atvas ao jogar. Esutods na NH e NE, indicam
	que a obesidade tende a diminuir desde que se conheçam os riscos
	da obesidade. AS companhias de jogos investem recursos significantes
	para identificar os múltiplos fatores que influecnai a escolha do
	jogo. Mas como promover a saúde através de jogos para crianças? É
	importante avaliar estratégias para reproduzir o conhecimento e traduzir
	as perspectivas. Uma das respostas pode ser o envolvimneto da família.
	Enfatizando nos aspectos sociais do jogo, incluidno competição e
	camaradagem podem ser efetivos para o uso. Biddis e Irwin afirmam
	que a diversão é o prinicipal aspecto motivacional para a participação
	de ativdades físicas. Jogos como farmville e second life, podem prover
	requisitos para o desenvolvimento de jogos que atamquem a obesidade
	infantil.},
  timestamp = {2012.02.27},
  url = {http://dx.doi.org/10.4061/2011/179124}
}

