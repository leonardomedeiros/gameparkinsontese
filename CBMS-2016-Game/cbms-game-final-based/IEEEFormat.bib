@INPROCEEDINGS{Hardy2011,
  author = {Hardy, S. and El Saddik, A. and Gobel, S. and Steinmetz, R.},
  title = {Context aware serious games framework for sport and health},
  booktitle = {Medical Measurements and Applications Proceedings (MeMeA), 2011 IEEE
	International Workshop on},
  year = {2011},
  pages = {248 -252},
  month = {may},
  abstract = {Serious Games for Sport and Health are a promising chance to help
	people to improve their health. In this paper, we address the challenges
	of adding context awareness to Serious Games for Sport and Health.
	We propose a framework for social networks and web services that
	is specialized in capturing temporal and spatial context as well
	as vital parameters of a user. Our framework dynamically maps necessary
	e-Health services such as exergames. In this way, we assist a user
	to find the most appropriate health services according to his/her
	needs at anytime and anywhere. We present our initial proof of concept
	implementation of the framework, which includes new sensor based
	Serious Games for Sport and Health as well as our test results.},
  doi = {10.1109/MeMeA.2011.5966775},
  keywords = {Web services;context aware serious games framework;e-Health services;social
	networks;sport;Web services;biomedical communication;health care;medical
	computing;sport;ubiquitous computing;},
  owner = {leonardo},
  review = {O autor apresenta um framework para construir informação baseada em
	contexto chamando de SocialAware. Esse framework foi testado numa
	aplicaçaõ de prova de conceito. Estudos multidisciplinares recentes,
	tem pesquisado no domínio de redes sensores sem-fio, smart phones,
	redes sociais e comunicação móvel. Esses estudos contribuíram em
	capturar sinais vitais como: batimento cardíaco, pressão sanguínea,
	nível de glicose e etc. Além disso, atividades como andar, dormir,
	dirigir, cair, correr falar e conversar com um amigo pode ser capturado
	assim como variáveis de ambiente como temperatura, humidade, localização,
	altitude e etc. Nesse artigo é apresentado uma integração com um
	conjunto de jogos sérios para o suporte ao esporte e a saúde e é
	utilizado a informação de contexto para o framework. Extrair informação
	de contexto através de sensores em serviços heterogêneos trás dois
	desafios. Primeiramente, requer extrair conteúdo de serviços distintis
	e análise de BSN. Segundo, extrair informação de redes sociais agregar
	e armazenar no repositório específico e extrair informação relevante
	num jogo sério ou serviço de saúde. Módulo de Extração de Conteúdo
	===================== This module takes part in the actual content
	extraction process from heterogeneous Internet-based services and
	body sensor network. Módulo de Extração de Contexto =====================
	Esse módulo emprega o padrão de design MVC2 para encontrar valores
	de contexto baseados em serviço. O Context Extrator extrai informações
	da BSN e dos serviços através de seus controladores. Módulo de Serviço
	de Adaptação Dinâmica =========================== Para compor as
	primitivas de contexto no desenvolvimento do portal de rede social
	chamado SenseFace, é utilizado para mapear um subconjunto de jogos
	sérios e/ou serviços de saúde. E através da Ubiquity Stack provê
	uma subcamada de rede ubíqua para cada pessoa. Essa pilha provê a
	lógica, protocolos e algoritmos para extrair as informações de esporte
	e saúde dos dados personalizados dos serviços. Implementação ==========
	Devido a todas as camadas da pilha serem implementadas em PHP, foi
	usado um framework PHP chamado XAMPP, que atua diretamente no servidor
	WEB. A Gui foi implementada usando um Web Portal open Source denominado
	iMoogle, que utiliza PHP e JSON. O sistema armazena as informações
	vitais [17, 15, 21] como sensores foram utilizados po relógio Ez-430
	Chronos Sports e a bicicleta ergométrica Daum 8008 TRS 3. A partir
	desses dispositivos foi possível mensurar a atividade física de uma
	pessoa. Foi implementado uma interface RESTful para transmitir os
	dados do acelerômetro em tempo real [15]. Dessa maneira foi possível
	detectar longos períodos de calmaria e de exercício físico. A interface
	RESTful permite sugerir a execução de atividades como caminhar, jogar
	determinado game, pilates. Test Results ======== Para avaliar a efetividade
	do sistema proposta foram conduzidos uma amostra de 95 pessas 25%
	de mulheres e conduziram entre 15 de maio de 2010 a 30 de novembro
	de 2010. A maior dos usuários possuíam mais de 25 anos e 45 usuários
	menos de 25 anos. Todos os participantes utilizam serviço de internet
	em diferentes escalas nos útlims 4 a 6 anos. A idéia do serviço dinâmico
	sugere que os exercícios físicos sugeridos através do jogo sério
	foi fortemente aceitado por 65% dos usuários, 30% consentiram e somente
	5% acharam inapropriado. Conclusão ======= O autor demonstrou que
	é possível desenvolver uma aplicação que use diferentes tecnologias
	para capturar o contexto e aplicá-lo para atividades físicas. E que
	essa aplicação foi aceita pela maioria dos usuários que acharam adequado
	praticar exercício físicos em determinados momentos. Inglês ====
	The author presents a framework for building context-aware services,
	called Social Aware. The framework has been tested with two proof-of-concept
	applications. Recent advancements in multidisciplinary research domains
	such as wireless sensors, smartphones, high speed personal area networking
	and mobile communication have contributed to a new era of context-aware
	computing. Extracting user context from sensory data and multimedia
	information contained within heterogeneous services poses two challenges.
	First, it requires live extraction of multimedia content from heterogeneous
	services and from BSN for context analysis. Second, employing appropriate
	logic to extract contextual information from the available social
	network and sensory data, aggregate and then store them to a repository
	to be used for dynamic selection of relevant serious games and health
	services. For better representation of the concept, we describe the
	framework with three logical modules. The first module deals with
	the extraction of multimedia data from different services over the
	web and live sensory data from one’s BSN},
  timestamp = {2011.12.01}
}

